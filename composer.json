    "dnafactory",
    "teamwork"
  ],
  "homepage": "https://github.com/dnafactory/laravel-teamwork-api",
  "license": "MIT",
  "require": {
    "php": ">=7.4.0",
    "illuminate/support": "^v8.0.0",
    "illuminate/routing": "^v8.0.0",
    "illuminate/config": "^v8.0.0",
    "dnafactory/teamwork-api": "^2.4.0"
  },
  "authors": [
    {
      "name": "Daniel Russo",
      "email": "daniel@dnafactory.it",
      "role": "Developer"
    },
    {
      "name": "Domenico Ragusa",
      "email": "domenico.ragusa@dnafactory.it",
      "role": "Developer"
    }
  ],
  "autoload": {
    "psr-4": {
      "DNAFactory\\Teamwork\\": ""
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "DNAFactory\\Teamwork\\TeamworkServiceProvider"
      ]
    }
  },
  "prefer-stable": false
}
